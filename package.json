{
  "author": {
    "name": "vo0doo",
    "email": "admin@stgrib.ru",
    "url": "https://blog.vo.dedyn.io"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "add-module-exports"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "16.15.1"
          }
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/vo0doo/ghost-timeweb-storage-adapter-s3/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.1035.0",
    "dotenv": "^16.0.1",
    "ghost-storage-base": "0.0.1",
    "rxjs": "^7.5.5"
  },
  "description": "An Timeweb S3 storage adapter for Ghost 0.10+",
  "devDependencies": {
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "nodemon": "^2.0.16",
    "semantic-release": "^15.9.9",
    "snazzy": "^5.0.0",
    "standard": "^8.3.0",
    "travis-deploy-once": "^5.0.2"
  },
  "homepage": "https://github.com/vo0doo/ghost-timeweb-storage-adapter-s3#readme",
  "license": "ISC",
  "keywords": [
    "adapter",
    "aws",
    "ghost",
    "s3",
    "storage"
  ],
  "main": "index.js",
  "name": "ghost-timeweb-storage-adapter-s3",
  "repository": {
    "type": "git",
    "url": "https://github.com/vo0doo/ghost-timeweb-storage-adapter-s3.git"
  },
  "scripts": {
    "debug": "npx nodemon",
    "build": "npx nodemon babel ./src/timeweb-vo0doo.js --out-file ./timeweb-vo0doo-run.js",
    "commit": "git-cz",
    "fix": "standard --fix",
    "lint": "standard --verbose | snazzy",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release",
    "test": "exit 0",
    "travis-deploy-once": "travis-deploy-once"
  },
  "version": "0.0.1-development"
}
